<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>Classic Movies</name>
    <match>all</match>
    <rule field="year" operator="before">2000</rule>
    <rule field="genre" operator="contain">anima</rule>
    <limit>all</limit>
    <order>title</order>
</smartplaylist>
