<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="tvshows">
    <name>1940s</name>
    <match>all</match>
    <rule field="year" operator="after">1939</rule>
    <rule field="year" operator="before">1950</rule>
    <rule field="genre" operator="doesnotcontain">anime</rule>
    <rule field="genre" operator="doesnotcontain">anima</rule>
    <limit>all</limit>
    <order direction="ascending">year</order>
</smartplaylist>
