<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="tvshows">
    <name>Unwatched TV shows</name>
    <match>all</match>
    <rule field="inprogress" operator="false" />
    <rule field="numwatched" operator="is">0</rule>
    <rule field="numepisodes" operator="greaterthan">0</rule>
    <limit>50</limit>
    <order direction="descending">dateadded</order>
</smartplaylist>
