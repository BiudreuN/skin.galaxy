<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>2000s</name>
    <match>all</match>
    <rule field="year" operator="after">1999</rule>
    <rule field="year" operator="before">2010</rule>
    <rule field="genre" operator="contain">anime</rule>
    <limit>all</limit>
    <order direction="ascending">year</order>
</smartplaylist>
