<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="tvshows">
    <name>Anime Tv Shows</name>
    <match>one</match>
    <rule field="genre" operator="contains">
        <value>Anime</value>
    </rule>
    <rule field="tag" operator="contains">
        <value>Anime</value>
    </rule>
    <limit>all</limit>
    <order direction="ascending">random</order>
</smartplaylist>
