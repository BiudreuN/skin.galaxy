<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="tvshows">
    <name>2020s</name>
    <match>all</match>
    <rule field="year" operator="after">2019</rule>
    <rule field="year" operator="before">2030</rule>
    <rule field="genre" operator="contain">anime</rule>
    <limit>all</limit>
    <order direction="ascending">year</order>
</smartplaylist>
