<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>Comedy Movies</name>
    <match>all</match>
    <rule field="genre" operator="contain">
        <value>comedy</value>
        <value>comedia</value>
    </rule>
    <limit>all</limit>
    <order>random</order>
</smartplaylist>
