<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>Películas de Acción</name>
    <match>all</match>
    <rule field="tag" operator="contain">
        <value>crime</value>
        <value>criminal</value>
        <value>detective</value>
    </rule>
    <rule field="genre" operator="doesnotcontain">anime</rule>
    <rule field="genre" operator="doesnotcontain">anima</rule>
    <limit>all</limit>
    <order>title</order>
</smartplaylist>
