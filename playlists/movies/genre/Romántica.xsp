<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>Romance</name>
    <match>all</match>
    <rule field="genre" operator="contain">
        <value>rom</value>
    </rule>
    <rule field="genre" operator="doesnotcontain">anime</rule>
    <rule field="genre" operator="doesnotcontain">anima</rule>
    <limit>all</limit>
    <order>title</order>
</smartplaylist>
