<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="tvshows">
    <name>2010s</name>
    <match>all</match>
    <rule field="year" operator="after">2009</rule>
    <rule field="year" operator="before">2020</rule>
    <rule field="genre" operator="contain">anima</rule>
    <limit>all</limit>
    <order direction="ascending">year</order>
</smartplaylist>
