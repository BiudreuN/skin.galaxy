<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>1950s</name>
    <match>all</match>
    <rule field="year" operator="after">1949</rule>
    <rule field="year" operator="before">1960</rule>
    <rule field="genre" operator="contain">anima</rule>
    <limit>all</limit>
    <order direction="ascending">year</order>
</smartplaylist>
